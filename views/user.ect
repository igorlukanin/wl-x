<% extend 'layout.ect' %>

<pre><%= @user.name %>, <%= @user.email %></pre>

<pre>
<% for @task_ in @tasks: %>
<% if @task_.task.completed: %>+<% else: %>-<% end %> <%= @task_.task.title %> / <%= @task_.list.title %>

<% for @subtask in @task_.task.subtasks: %>
    <% if @subtask.completed: %>+<% else: %>-<% end %> <%= @subtask.title %>

<% end %>

<% end %>
</pre>